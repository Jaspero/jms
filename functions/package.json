{
  "name": "functions",
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "copy:definitions": "rm -rf definitions && node copy-definitions.js",
    "build:definitions": "npm run --prefix ../definitions build && npm run copy:definitions",
    "build": "tsc",
    "watch": "npm run build -- -w",
    "serve": "npm run build && firebase serve --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "config": "firebase functions:config:get > .runtimeconfig.json"
  },
  "engines": {
    "node": "16"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@google-cloud/storage": "5.8.3",
    "@jaspero/utils": "1.7.1",
    "@sendgrid/mail": "7.7.0",
    "adv-firestore-functions": "2.0.9",
    "ajv": "8.11.0",
    "busboy": "0.3.1",
    "cors": "2.8.5",
    "cors-anywhere": "0.4.4",
    "csvtojson": "2.0.10",
    "definitions": "file:definitions",
    "express": "4.18.2",
    "firebase-admin": "11.5.0",
    "firebase-functions": "4.2.0",
    "handlebars": "4.7.7",
    "http-proxy": "1.18.1",
    "json-pointer": "0.6.2",
    "json2csv": "5.0.7",
    "jsonwebtoken": "9.0.0",
    "proxy-from-env": "1.1.0",
    "sharp": "0.31.3",
    "xlsx": "0.18.5"
  },
  "devDependencies": {
    "@types/ajv": "1.0.0",
    "@types/busboy": "0.2.3",
    "@types/cors": "2.8.6",
    "@types/csvtojson": "1.1.5",
    "@types/express": "4.17.1",
    "@types/handlebars": "4.1.0",
    "@types/json-pointer": "1.0.30",
    "@types/json-schema": "7.0.11",
    "@types/json2csv": "5.0.1",
    "@types/jsonwebtoken": "8.5.6",
    "@types/sharp": "0.30.4",
    "tslint": "6.1.2",
    "typescript": "4.8.2"
  }
}
