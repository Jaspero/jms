    "eslint-plugin-svelte3": "4.0.0",
    "svelte": "3.48.0",
    "svelte-check": "2.7.1",
    "svelte-preprocess": "4.10.6",
    "tslib": "2.4.0",
    "typescript": "4.6.4"
  },
  "type": "module",
