{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "es2015",
      "dom"
    ],
    "moduleResolution": "node",
    "jsx": "react",
    "reactNamespace": "JSX",
    "noImplicitAny": false,
    "strictNullChecks": false,
    "declaration": true,
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "composite": true,
    "types": ["node"]
  },
  "exclude": ["setup.ts", "dist"]
}
